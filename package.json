{
  "name": "mise-en-place",
  "version": "0.0.5",
  "description": "A collection of layout primitives for building Vue.js apps.",
  "keywords": [
    "vue",
    "vue.js",
    "javascript",
    "css",
    "layout",
    "grid"
  ],
  "author": {
    "name": "Phillip Curl",
    "email": "phillipcurl@gmail.com",
    "github": "phillipcurl"
  },
  "license": "MIT",
  "homepage": "https://phillipcurl.com/work/mise-en-place",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phillipcurl/mise-en-place.git"
  },
  "bugs": {
    "url": "https://github.com/phillipcurl/mise-en-place/issues"
  },
  "main": "dist/mise-en-place.ssr.js",
  "module": "dist/mise-en-place.esm.js",
  "unpkg": "dist/mise-en-place.min.js",
  "browser": {
    "./ssr": "src/lib/entry.js"
  },
  "files": [
    "dist/*",
    "lib/**/*.vue"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build-demo": "vue-cli-service build",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "release": "np"
  },
  "dependencies": {
    "@styled-system/core": "^5.1.2",
    "@styled-system/css": "^5.1.4",
    "@styled-system/variant": "^5.1.4",
    "deepmerge": "^4.2.2",
    "emotion": "^10.0.27"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.5",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "@vuepress/plugin-register-components": "^1.0.0-rc.1",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.3.2",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.4.2",
    "minimist": "^1.2.0",
    "np": "^5.1.3",
    "postcss-css-variables": "^0.13.0",
    "prettier": "^1.18.2",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "5.1.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.2.0",
    "vuex": "^3.0.1"
  },
  "peerDependencies": {
    "vue": ">=2.0.0"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "np": {
    "contents": "dist"
  }
}
